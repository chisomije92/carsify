{
  "version": 2,
  "env": {
    "MONGO_URI": "@MONGODB_URI",
    "COOKIE_KEY": "@COOKIE_KEY"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "DELETE", "PATCH"]
    }
  ]
}
